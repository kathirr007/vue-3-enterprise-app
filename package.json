{
  "name": "brightreturn-web-ui",
  "type": "module",
  "version": "1.9.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite --force --host",
    "build": "run-p type-check build-only",
    "preview": "pnpm build-only && vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:tsc": "vue-tsc --noEmit",
    "lint:fix-eslint": "eslint . --cache --fix",
    "lint:fix-stylelint": "stylelint \"src/**/*.{css,scss,vue}\" --cache --fix",
    "format": "prettier ./src -write",
    "prepare": "husky install",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev",
    "mobile:build": "VITE_API_ENDPOINT=https://app.brightreturn.com/api/v1 pnpm build && pnpm cap sync && pnpm cap copy",
    "mobile:android": "pnpm build && pnpm cap sync && pnpm cap copy"
  },
  "dependencies": {
    "@capacitor/preferences": "^5.0.7",
    "@capacitor/push-notifications": "^5.1.1",
    "@fullcalendar/core": "6.1.8",
    "@fullcalendar/daygrid": "6.1.8",
    "@fullcalendar/interaction": "6.1.8",
    "@fullcalendar/vue3": "6.1.8",
    "@nangohq/frontend": "^0.25.8",
    "@novu/notification-center-vue": "^0.16.3",
    "@types/papaparse": "5.3.7",
    "@types/quill": "^2.0.14",
    "@vueform/multiselect": "^2.6.6",
    "@vueform/vueform": "^1.6.6",
    "@vueuse/core": "10.7.0",
    "@vueuse/head": "2.0.0",
    "@vueuse/integrations": "10.7.0",
    "@vueuse/router": "10.7.0",
    "awesome-phonenumber": "5.11.0",
    "axios": "1.4.0",
    "chart.js": "4.3.0",
    "dayjs": "1.11.9",
    "dayjs-business-days2": "1.2.1",
    "floating-vue": "2.0.0-beta.24",
    "libphonenumber-js": "^1.10.57",
    "mitt": "^3.0.1",
    "nanoid": "^5.0.3",
    "nprogress": "0.2.0",
    "papaparse": "5.4.1",
    "paperizer": "^0.1.6",
    "pretty-bytes": "6.1.0",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primevue": "^3.44.0",
    "pspdfkit": "^2023.4.0",
    "quill": "1.3.7",
    "quill-placeholder-module": "^0.3.1",
    "secure-ls": "^1.2.6",
    "universal-cookie": "4.0.4",
    "unplugin-vue-components": "0.25.1",
    "validator": "^13.11.0",
    "vee-validate": "4.6.7",
    "vite-plugin-pages": "0.32.0",
    "vite-plugin-vue-layouts": "0.11.0",
    "vue": "3.4.21",
    "vue-query": "1.26.0",
    "vue-router": "4.2.4",
    "vue-star-rating": "^2.1.0",
    "vue3-scroll-spy": "1.0.8",
    "vuedraggable": "^4.1.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@capacitor/android": "5.6.0",
    "@capacitor/app": "5.0.6",
    "@capacitor/assets": "3.0.4",
    "@capacitor/cli": "5.6.0",
    "@capacitor/core": "5.6.0",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.6.0",
    "@capacitor/keyboard": "5.0.7",
    "@capacitor/splash-screen": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@eslint/eslintrc": "^3.0.1",
    "@iconify/vue": "4.1.1",
    "@popperjs/core": "^2.11.8",
    "@stylistic/stylelint-config": "^1.0.1",
    "@types/papaparse": "5.3.7",
    "@types/validator": "^13.11.2",
    "@unocss/eslint-plugin": "^0.58.5",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/tsconfig": "0.4.0",
    "add": "2.0.6",
    "commit-and-tag-version": "11.2.2",
    "cordova-res": "0.15.4",
    "eslint": "npm:eslint-ts-patch@8.56.0-0",
    "eslint-ts-patch": "8.56.0-0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "1.63.6",
    "stylelint": "^16.2.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^6.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.3.3",
    "unocss": "0.53.5",
    "unplugin-auto-import": "0.16.6",
    "vite": "4.4.9",
    "vue-tsc": "2.0.5"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache --fix",
    "*.{css,scss,vue}": "stylelint --cache --fix"
  }
}
